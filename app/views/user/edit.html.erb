<h1> Edit profile </h1>
<%= form_for @user, :url => url_for(:controller => 'user', :action => 'edit') do |f| %>
<p> <%= f.label 'Bio:' %>
  <%= f.text_area :bio, class: "form-control"%> </p>
<p> <%= f.label 'Contact Email:' %>
  <%= f.email_field :contact, class: "form-control"%> </p>
<p> <%= f.label 'Height (inches):' %>
  <%= f.text_field :height, class: "form-control", type: "number"%> </p>
 <%= f.submit %>
<% end %>
